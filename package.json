{
  "name": "cz-conventional-changelog-jira-first",
  "version": "0.2.0",
  "description": "This is a typeScript implementation of a commitizen adapter for conventional changelogs, with the jira id as first item in the header.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "commit": "git cz",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "prepare": "husky install",
    "prepublish": "tsc",
    "test": "echo \"No test specified\"",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Michriko/cz-conventional-changelog-jira-first.git"
  },
  "keywords": [
    "commit",
    "pretty",
    "format",
    "commitizen",
    "conventional changelog",
    "jira",
    "commitlint"
  ],
  "author": "Michael Kopf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Michriko/cz-conventional-changelog-jira-first/issues"
  },
  "homepage": "https://github.com/Michriko/cz-conventional-changelog-jira-first#readme",
  "engines": {
    "npm": ">=5.8.0",
    "node": ">=10.15.2"
  },
  "dependencies": {
    "@types/git-branch": "^2.0.1",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.168",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.4",
    "conventional-commit-types": "^3.0.0",
    "git-branch": "^2.0.1",
    "lodash": "^4.17.21",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^12.1.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "commitlint": "^12.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "release-it": "^14.6.2",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
